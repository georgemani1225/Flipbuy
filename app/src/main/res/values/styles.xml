<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/OurColor2</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/OurColor2</item>
    </style>

    <style name="welcome_bac">

        <item name="android:textSize">
            22sp
        </item>

        <item name="android:textColor">
            #7AC9D0
        </item>

    </style>


    <style name="ToolBarThemeRegister" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/OurColor2</item>
        <item name="colorPrimaryDark">#005059</item>
        <item name="titleTextColor">@color/OurColor4</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textColorSecondary">@color/OurColor2</item>
    </style>

    <style name="ToolBar.TitleText" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">18sp</item>
    </style>

</resources>
